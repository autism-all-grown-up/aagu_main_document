@article{LodiSmith2021,
  title = {The Codevelopment of Generativity and Well-Being into Early Late Life},
  author = {{Lodi-Smith}, Jennifer and Ponterio, Elyse J. and Newton, Nicky J. and Poulin, Michael J. and Baranski, Erica and Whitbourne, Susan Krauss},

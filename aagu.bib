@article{Grant2021,
  title = {Considering the {{Autistic}} Advantage in Qualitative Research: {{The}} Strengths of {{Autistic}} Researchers},
  shorttitle = {Considering the {{Autistic}} Advantage in Qualitative Research},
  author = {Grant, Aimee and Kara, Helen},
